#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules/typescript-esbuild/Target/node_modules:/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules/typescript-esbuild/node_modules:/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules:/mnt/d/Developer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules/typescript-esbuild/Target/node_modules:/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules/typescript-esbuild/node_modules:/mnt/d/Developer/node_modules/.pnpm/typescript-esbuild@0.1.12/node_modules:/mnt/d/Developer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../Target/Bin.js" "$@"
else
  exec node  "$basedir/../../Target/Bin.js" "$@"
fi
